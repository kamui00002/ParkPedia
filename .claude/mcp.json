{
    "mcpServers": {
      "firebase": {
        "command": "npx",
        "args": [
          "-y",
          "firebase-tools@latest",
          "mcp"
        ],
        "env": {}
      },
      "serena": {
        "command": "uvx",
        "args": [
          "--from",
          "git+https://github.com/oraios/serena",
          "serena-mcp-server",
          "--context",
          "ide-assistant"
        ],
        "env": {}
      },
      "context7": {
        "command": "npx",
        "args": [
          "-y",
          "@upstash/context7-mcp"
        ],
        "env": {}
      },
      "playwright": {
        "command": "npx",
        "args": [
          "-y",
          "@playwright/mcp@latest"
        ],
        "env": {}
      },
      "chrome-devtools": {
        "command": "npx",
        "args": [
          "-y",
          "chrome-devtools-mcp@latest"
        ],
        "env": {}
      }
    }
  }
  